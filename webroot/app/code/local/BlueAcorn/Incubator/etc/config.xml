<?xml version="1.0"?>
        <!-- The root node for Magento module configuration -->
<config>
    <modules>
        <BlueAcorn_Incubator>
            <version>1.0.0.7</version>
        </BlueAcorn_Incubator>
    </modules>
    <!-- Configure our module's behavior in the global scope -->
    <global>
        <models>
            <blueacorn_incubator>
                <class>BlueAcorn_Incubator_Model</class>
                <resourceModel>new_resource</resourceModel>
            </blueacorn_incubator>
            <new_resource>
                <class>BlueAcorn_Incubator_Model_Resource</class>
            </new_resource>
        </models>
        <resources>
            <blueacorn_incubator_setup>
                <setup>
                    <module>BlueAcorn_Incubator</module>
                </setup>
            </blueacorn_incubator_setup>
        </resources>
    </global>
    <frontend>
        <layout>
            <updates>
                <blueacorn_incubator>
                    <file>blueacorn/incubator-styling.xml</file>
                </blueacorn_incubator>
            </updates>
        </layout>
    </frontend>
</config>